  <main>
    <div class="px-5 mx-auto">
      <div class="flex flex-col">
        <div class="flex flex-wrap sm:flex-row flex-col py-8 mt-5">
          <h1 class="sm:w-2/5 text-gray-900 font-medium title-font text-4xl mb-2 sm:mb-0 font-mono">Featured Articles</h1>
        </div>
      </div>
    </div>
    <section class="text-gray-600 body-font">
      <div class="px-5 mx-auto">
        <div class="flex flex-wrap -mx-4 -mb-10 text-center">
          <% var i = 0;%>
            <% articles.forEach(article => { if(i < 2) { %>
              <a href="articles/<%= article.slug %>">
          <div class="sm:w-1/2 mb-10 px-4 shadow-sm rounded-xl">
            <div class="rounded-lg h-80 overflow-hidden">
              <img alt="content" class="object-cover object-center h-full w-full transition transform ease-in-out duration-100 hover:scale-105" src="<%= article.imageUrl %>">
            </div>
            <h2 class="tracking-widest text-xs title-font font-medium text-gray-400 my-2"><%= article.category %></h2>
            <h2 class="title-font text-2xl font-medium text-gray-900 mb-3 transition transform ease-in-out duration-100 hover:scale-105"><%= article.title %></h2>
            <p class="leading-relaxed mb-3"><%= article.createdAt.toLocaleDateString() %></p>
            <a href="articles/<%= article.slug %>" class="text-blue-500 inline-flex items-center transition transform ease-in-out duration-100 hover:scale-105">Read -></a>
          </div>
        </a>
          <% } i++; }) %>
        </div>
      </div>
    </section>
    <section class="text-gray-600 body-font">
      <div class="px-5 py-18 mx-auto">
        <div class="flex flex-col">
          <div class="flex flex-wrap sm:flex-row flex-col py-6 mt-5">
            <h1 class="sm:w-2/5 text-gray-900 font-medium title-font text-4xl mb-2 sm:mb-0 font-mono">More Articles</h1>
          </div>
        </div>
        <div class="container">
          <div id="searchWrapper">
            <input class="mb-10 font-mono w-full" type="text" name="searchBar" id="searchBar" placeholder="Search Articles by Title or Category" />
          </div>
        </div>
        <div id="charactersList" class="flex flex-wrap sm:-m-4 -mx-4 -mb-10 -mt-4">
        </div>
      </div>
    </section>
  </main>
